{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "extends": "airbnb",
  "plugins": [
    "import",
    "react",
    "jsx-a11y"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/no-unused-prop-types": 1,
    "react/forbid-prop-types": 1,
    "jsx-a11y/anchor-is-valid": 0
  }

}
